<%- include("../partials/head.ejs")%>




        <div class="col-12">
            <div class="container p-3 w-50">
                <h1 class="text-center mt-5 mb-3">Modificar Usuario</h1>
                <%- include('../partials/messages') -%>
                    <form action="/edituser/<%= usuario.id%>?_method=PUT" method="POST">

                        <div class="form-group px-0 mt-5 mb-3">
                            <input value="<%= usuario.nombre %>" type="text" name="nombre" class="form-control"
                                placeholder="Nombre">
                        </div>

                        <div class="form-group px-0 mt-5 mb-3">
                            <input value="<%= usuario.email %>" type="email" name="email" class="form-control"
                                placeholder="Email">
                        </div>
                        <div class="d-flex justify-content-center mt-5 mb-3">
                            <button type="submit" class="btn btn-outline-dark btn-primary">Actualizar</button>
                        </div>


                    </form>
            </div>
        </div>
